<html title="Login"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd">
	<head>
		<link href="${context:mybootstrap/css/bootstrap.min.css}" rel="stylesheet"
			type="text/css" />
		<link href="${context:mybootstrap/css/main.css}" rel="stylesheet"
			type="text/css" />
	</head>
	
	<body class="theme-default">
		<section class="page-content">
			<div class="page-content-inner"
				style="background-image: url(${context:/images/4.jpg})">
				<!-- Login Page -->
				<div class="single-page-block-header">
					<div class="row">
						<div class="col-lg-4">
							<div class="logo">
								<a href="javascript: history.back();">
									<img src="${context:/mybootstrap/img/logo.png}" alt="Clean UI Admin Template" />
								</a>
							</div>
						</div>
					</div>
				</div>
				<div class="single-page-block">
					<div class="single-page-block-inner effect-3d-element">
						<div class="blur-placeholder"><!-- -->
						</div>
						<div class="single-page-block-form">
							<h3 class="text-center">
								<i class="icmn-enter margin-right-10"></i>
								Login
							</h3>
							<br />
							<br />
							<!-- <div class="row"> -->
							<!-- <div class="span4 offset3"> -->
							<t:form t:id="login">
								<div class="form-group">
									<t:textfield t:id="email" class="form-control"
										validate="required" placeholder="Email address" />
								</div>
								<div class="form-group">
									<t:passwordfield t:id="password" class="form-control password"
										validate="required" placeholder="Password" />
								</div>
								<div class="form-actions">
									<t:submit class="btn btn-primary width-150" value="Sign in" />
								</div>
							</t:form>
							<!-- </div> -->
							<!-- </div> -->
						</div>
					</div>
				</div>
			</div>

		</section>

		<div class="main-backdrop">
		</div>
	</body>
</html>
