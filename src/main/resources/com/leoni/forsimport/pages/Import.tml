<html t:type="layout" title="application Index"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">

	<div class="content-area p-y-1">
		<div class="container-fluid">
			<div class="box box-block bg-white">
				<div class="row">
					<t:form class="form-inline">
						<div class="col-md-6 offset-md-3">

							<t:errors />
							<!-- <t:if test="developer"> -->
							<h4>${message:select_table_name}</h4>
							<!-- <input t:type="textfield" class="form-control" t:id="table" -->
							<!-- value="tableName" validate="required" /> -->
							<!-- <div class="col-sm-3"> -->
							<t:textfield t:id="tableName" t:mixins="autocomplete"
								placeholder="table name" />
							<!-- </div> -->
							<!-- </t:if> -->
							<h4>${message:select_file}</h4>
							<input t:type="upload" class="dropify" t:id="file" t:value="file"
								validate="required" />
								<br/>
								<br/>
								
							<input t:type="submit"  t:id="upload"
								t:value="Upload" />
						</div>
					</t:form>
				</div>
			</div>
		</div>
	</div>
</html>
