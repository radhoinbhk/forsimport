<html t:type="layout" title="application Index"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">

	<!-- Most of the page content, including <head>, <body>, etc. tags, comes 
		from Layout.tml -->

	<!-- Main hero unit for a primary marketing message or call to action -->

	<t:form class="form-inline">
		<div class="form-group">
			<t:errors />
			<h3>${message:select_table_name}</h3>
			<input t:type="textfield" class="form-control" t:id="table"
				value="tableName" validate="required"/>
			<h3>${message:select_file}</h3>
			<input t:type="upload" class="form-control" t:id="file"
				t:value="file" validate="required" />
			<input t:type="submit" class="form-control" t:id="upload"
				t:value="Upload" />
		</div>
	</t:form>
</html>
