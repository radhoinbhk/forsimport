	<body>
		<!-- navbar navbar-inverse navbar-fixed-top -->
		<div class="site-sidebar" role="navigation">
			<a class="logo" href="#">
<!-- 				<span class="l-text">LEONI</span> -->
<!-- 				<span class="l-icon"></span> -->
				<div><img src="images/logo.png" alt="" /></div>
			</a>
			<div class="container">
				<div class="navbar-header">
