			rel="stylesheet" type="text/css" />
		<!-- Neptune CSS -->
		<link href="${context:mybootstrap/css/core.css}" rel="stylesheet"
			type="text/css" />

		<!-- vendor Styles -->
		<!-- v1.0.0 -->
		<link
			href="${context:mybootstrap/vendor/bootstrap/dist/css/bootstrap.min.css}"
			rel="stylesheet" type="text/css" />
		<!-- Clean UI Styles -->
		<link href="${context:mybootstrap/common/css/source/main.css}"
			rel="stylesheet" type="text/css" />
		<!-- vendor Scripts -->
		<!-- v1.0.0 -->
		<script src="/web/src/main/webapp/mybootstrap/vendor/jquery/jquery.min.js"></script>
		<script
			src="/web/src/main/webapp/mybootstrap/vendor/bootstrap/dist/js/bootstrap.min.js"></script>
		<script
